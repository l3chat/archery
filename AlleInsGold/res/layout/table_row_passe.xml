<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tableRow_standard_passe_row"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:weightSum="13" >

    <TextView
        android:id="@+id/passeRow_rowNumber"
        android:layout_weight="1"
        android:text="@string/placeholder1"
        android:textAppearance="@style/textAppearanceTiny" />

    <TextView
        android:id="@+id/passeRow_arrow1_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/passeRow_arrow2_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/passeRow_arrow3_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/passeRow_sum1_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/passeRow_sum2_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/passeRow_carry_value"
        android:layout_weight="2"
        android:text="@string/placeholder0"
        android:textAppearance="?android:attr/textAppearanceMedium" />
    
</TableRow>